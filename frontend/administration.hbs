<html>
<head>
    <title>Administration</title>
</head>
<body>
    <table>
        <tr>
            <th>
                User Emails
            </th>
            <th>
                Full Name
            </th>
            <th>
                Phone No.
            </th>
        </tr>
        {{#each users}}
            <tr>
                <td>
                    {{email}}
                </td>
                <td>
                    {{fullName}}
                </td>
                <td>
                    {{phoneNumber}}
                </td>
            </tr>
        {{/each}}
    </table>
</body>
</html>
